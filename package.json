{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@types/express": "^4.17.3",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/node": "^13.9.1",
    "@types/react-helmet": "^5.0.15",
    "@types/validator": "^12.0.1",
    "ajv": "^6.12.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dcl-crypto": "^2.1.0",
    "decentraland-crypto": "0.0.2",
    "decentraland-gatsby": "^1.12.3",
    "decentraland-server": "^2.11.6",
    "decentraland-ui": "^2.16.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gatsby": "^2.19.7",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "^2.1.30",
    "gatsby-plugin-sharp": "^2.4.3",
    "gatsby-plugin-typescript": "^2.2.3",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "isomorphic-fetch": "^2.2.1",
    "morgan": "^1.9.1",
    "node-pg-migrate": "^4.2.2",
    "nodemon": "^2.0.2",
    "pg": "^7.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3",
    "uuid": "^7.0.2",
    "validator": "^12.2.0",
    "web3x": "^4.0.6"
  },
  "devDependencies": {
    "prettier": "^1.19.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build && tsc -p .",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "concurrently -c blue,green -n SERVER,FRONT 'npm run serve' 'npm run develop'",
    "serve": "DOTENV_CONFIG_PATH=.env.development nodemon --watch src/entities --watch src/server.ts -e ts,json --exec 'ts-node -r dotenv/config.js' src/server",
    "clean": "gatsby clean",
    "migrate": "DOTENV_CONFIG_PATH=.env.development ts-node -r dotenv/config.js ./node_modules/node-pg-migrate/bin/node-pg-migrate -j ts -d CONNECTION_STRING",
    "production": "NODE_ENV=production node lib/server.js",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
